{
    "env": {
        "es2020": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-unused-vars": ["error", { "varsIgnorePattern": "Repository|Response|Request|MigrationInterface|QueryRunner"}],
        "no-param-reassign": 2
    }
}
